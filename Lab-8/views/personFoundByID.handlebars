<h1>Person Found</h1>
<h2>{{fullname}}</h2>

<main>
    <dl>
        <dt>ID: </dt>
        <dd>{{id}}</dd>
        <dt>First Name:</dt>
        <dd>{{fname}}</dd>
        <dt>Last Name:</dt>
        <dd>{{lname}}</dd>
        <dt>Address:</dt>
        <dd>{{add}}</dd>
        <dt>Zip Code:</dt>
        <dd>{{zip}}</dd>
        <dt>Phone:</dt>
        <dd>{{phone}}</dd>
        <dt>Social Security Number:</dt>
        <dd>{{ssn}}</dd>
    </dl>
</main>